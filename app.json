{
  "expo": {
    "name": "T Live Voice",
    "slug": "t-live-voice",
    "owner": "hiepnvna",
    "version": "1.0.0",
    "sdkVersion": "52.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "tlivevoice",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hiepnvna.tlivevoice",
      "buildNumber": "1.0.0",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hiepnvna.tlivevoice"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps._some_id_here_"
        }
      ],
      "expo-router",
      "expo-font",
      [
        "react-native-iap",
        {
          "paymentProvider": "both"
        }
      ],
      "react-native-screens",
      "./plugins/tlivevoice-background"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f6c47bf1-fdab-4e63-bd20-e626587ab54b"
      }
    },
    "newArchEnabled": true
  }
}